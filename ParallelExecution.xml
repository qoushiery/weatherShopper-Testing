<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="testSuite" parallel="tests" thread-count="2">
    <test name="TestOnChrome">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="com.weatherShopper.test.tests.TestWeatherShopperEndToEnd"/>
            <class name="com.weatherShopper.test.tests.TestBuyingLeastMoisturizerWhileTempBelow19"/>
            <class name="com.weatherShopper.test.tests.TestBuyingLeastSunscreensWhileTempAbove34"/>
        </classes>
    </test>
    <test name="TestOnEdge">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="com.weatherShopper.test.tests.TestWeatherShopperEndToEnd"/>
            <class name="com.weatherShopper.test.tests.TestBuyingLeastMoisturizerWhileTempBelow19"/>
            <class name="com.weatherShopper.test.tests.TestBuyingLeastSunscreensWhileTempAbove34"/>
        </classes>
    </test>
</suite>